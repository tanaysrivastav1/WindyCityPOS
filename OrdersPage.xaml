<Page x:Class="WindyCityPOS.RecordsWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WindyCityPOS"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RecordsWindow">

    <Grid>
        <StackPanel Margin="0,0,742,0" Background="#FF96B8CB">
            <Button Content="For-Here" Margin="0,20,0,0" Height="50" Width="50"/>
            <Button Content="Pick-Up" Margin="0,20,0,0" Height="50" Width="50"/>
            <Button Content="Delivery" Margin="0,20,0,0" Height="50" Width="50"/>
            <Button Content="Closed" Margin="0,20,0,0" Height="50" Width="50"/>
        </StackPanel>
    </Grid>
</Page>
